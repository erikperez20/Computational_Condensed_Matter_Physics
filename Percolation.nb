 CellChangeTimes->{{3.841251892858514*^9, 
  3.8412519007917233`*^9}},ExpressionUUID->"424f0561-82b9-4a15-bfe2-\
04a3ee582d1d"],

Cell[BoxData[
 RowBox[{
  RowBox[{"HoshenKapelman", "[", "lattice_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"l", "=", 
       RowBox[{"Length", "[", 
        RowBox[{"lattice", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ",", "clusterlabels", ",", 
      "padlattice", ",", "largestlabel", ",", "left", ",", "upper", ",", 
      "minlabel", ",", "maxlabel"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"clusterlabels", "=", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"l", ",", "l"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"padlattice", "=", 
      RowBox[{"ArrayPad", "[", 
       RowBox[{"lattice", ",", "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"largestlabel", "=", "0"}], 
     RowBox[{"(*", 
      RowBox[{"Label", " ", "name", " ", "storage"}], "*)"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Dimensions", "[", "padlattice", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      StyleBox["Do",
       FontSize->12], 
      StyleBox["[",
       FontSize->12], 
      StyleBox["\[IndentingNewLine]",
       FontSize->12], 
      RowBox[{
       RowBox[{
        StyleBox["If",
         FontSize->12], 
        StyleBox["[",
         FontSize->12], 
        RowBox[{
         RowBox[{
          StyleBox[
           RowBox[{"padlattice", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "j"}], "]"}], "]"}],
           FontSize->12], 
          StyleBox["\[Equal]",
           FontSize->12], "1"}], 
         StyleBox[",",
          FontSize->12], 
         StyleBox["\[IndentingNewLine]",
          FontSize->12], 
         RowBox[{
          StyleBox[
           RowBox[{"left", "=", 
            RowBox[{"padlattice", "[", 
             RowBox[{"[", 
