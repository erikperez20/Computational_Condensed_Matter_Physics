Cell[CellGroupData[{

Cell["Change Cluster label function", "Subsubsection",
 CellChangeTimes->{{3.8415059466798906`*^9, 
  3.8415059562757487`*^9}},ExpressionUUID->"61de1a7e-da39-4b03-bbd7-\
d2d2f9d57264"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ChangeLabel", "[", "clusterlabels_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "oldlabels", ",", "newlabels", ",", "newclusterlabels", ",", "newlabel", 
      ",", "auxclusterlabels", ",", "position"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "Create", " ", "an", " ", "array", " ", "of", " ", "the", " ", "unique", 
      " ", "cluster", " ", "increasing", " ", "values", " ", "of", " ", "our",
       " ", "cluster", " ", "matrix"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"oldlabels", "=", 
      RowBox[{"Union", "[", 
       RowBox[{"Flatten", "[", "clusterlabels", "]"}], "]"}]}], " ", ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Generate", " ", "a", " ", "random", " ", "shuffle", " ", "array", " ", 
       "of", " ", "values", " ", "from", " ", "1", " ", "to", " ", "len", 
       RowBox[{"(", "oldlabels", ")"}], " ", "except", " ", "for", " ", "the",
        " ", "0", " ", "value"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"newlabels", "=", 
      RowBox[{"RandomSample", "[", 
       RowBox[{
        RowBox[{"Range", "[", 
         RowBox[{"1", ",", 
          RowBox[{
           RowBox[{"Length", "[", "oldlabels", "]"}], "-", "1"}]}], "]"}], 
        ",", 
        RowBox[{
         RowBox[{"Length", "[", "oldlabels", "]"}], "-", "1"}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"newlabels", "=", 
      RowBox[{"Insert", "[", 
       RowBox[{"newlabels", ",", "0", ",", "1"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "If", " ", "our", " ", "cluster", " ", "matrix", " ", "is", " ", "full",
        " ", "of", " ", "zeros", " ", "do", " ", 
       RowBox[{"nothing", "."}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"oldlabels", "\[Equal]", 
        RowBox[{"{", "0", "}"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"newclusterlabels", "=", "clusterlabels"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
        "Extract", " ", "the", " ", "index", " ", "of", " ", "the", " ", 
         "oldlabels", " ", "and", " ", "map", " ", "them", " ", "to", " ", 
         "the", " ", "newlabels", " ", "array"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"newclusterlabels", "=", "\[IndentingNewLine]", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"position", "=", 
               RowBox[{
                RowBox[{
                 RowBox[{"Position", "[", 
                  RowBox[{"oldlabels", ",", "#"}], "]"}], "[", 
                 RowBox[{"[", "1", "]"}], "]"}], "[", 
                RowBox[{"[", "1", "]"}], "]"}]}], ";", 
              RowBox[{"newlabels", "[", 
               RowBox[{"[", "position", "]"}], "]"}]}], ")"}], "&"}], ",", 
           "\[IndentingNewLine]", "clusterlabels", ",", 
           RowBox[{"{", "2", "}"}]}], "]"}]}], ";"}]}], "\[IndentingNewLine]",
       "]"}], ";", "\[IndentingNewLine]", "newclusterlabels"}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{
  3.8415084445739484`*^9},ExpressionUUID->"c80e0f1e-b64d-499e-bb6e-\
93d28d65784c"]
}, Open  ]],

